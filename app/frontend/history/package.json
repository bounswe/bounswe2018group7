{
  "name": "material-kit-react",
  "version": "1.3.0",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "@material-ui/core": "3.1.1",
    "@material-ui/icons": "3.0.1",
    "antd": "^3.10.9",
    "bootstrap": "^4.1.3",
    "classnames": "2.2.6",
    "connected-react-router": "^4.5.0",
    "draft-js": "^0.10.5",
    "formsy-react": "^1.1.5",
    "formsy-react-components": "^1.0.0",
    "history": "4.7.2",
    "moment": "2.29.2",
    "node-sass-chokidar": "1.3.3",
    "notistack": "^0.3.7",
    "nouislider": "12.0.0",
    "npm-run-all": "4.1.3",
    "prop-types": "15.6.2",
    "react": "^16.5.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-avatar": "^3.4.6",
    "react-datetime": "2.15.0",
    "react-dom": "^16.5.2",
    "react-google-maps": "^9.4.5",
    "react-h5-audio-player": "^1.0.1",
    "react-image": "^1.5.1",
    "react-infinite-calendar": "^2.3.1",
    "react-measurements": "^0.6.4",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-slick": "0.23.1",
    "react-sound": "^1.2.0",
    "react-soundplayer": "^1.0.4",
    "react-swipeable-views": "0.13.0",
    "react-tag-input": "^5.2.3",
    "react-tagsinput": "^3.19.0",
    "react-yearly-calendar": "^1.2.1",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "serve": "^10.0.2",
    "universal-cookie": "^3.0.4",
    "video-react": "^0.13.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-import-rename": "1.0.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1"
  },
  "optionalDependencies": {
    "ajv": "5.0.0",
    "babel-eslint": "7.2.3",
    "eslint": "4.19.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.14.3"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "serve -s build",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "build-package-css": "node-sass-chokidar src/assets/scss/material-kit-react.scss dist/material-kit-react.css",
    "build-package": "npm run build-package-css && babel src --out-dir dist",
    "heroku-postbuild": "npm run build"
  }
}
